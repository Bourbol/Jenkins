#!Groovy

properties([disableConcurrentBuilds()])

pipeline {
    agent {
        label 'master'
    }
    options {
        timestamps()
    }
    stages {
        stage("Docker Build") {
            steps {
                echo "--------------- Start Docker Build ---------------"
                dir ('docker/') {
                    sh 'docker build .'
                }
            }
        }
    }
}